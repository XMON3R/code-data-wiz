{
  "name": "viz24",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "clean-lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean-lint-more": "eslint src/**/*.{js,ts,tsx}",
    "p-check": "prettier . --check",
    "pretty": "prettier --write ./src",
    "preview": "vite preview",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./src/output.css",
    "start:css": "npx tailwindcss -i ./src/index.css -o ./src/output.css --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@codemirror/lang-sql": "^6.8.0",
    "@pintora/cli": "^0.7.6",
    "@uiw/react-codemirror": "^4.23.7",
    "antlr4": "^4.13.2",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^15.0.4",
    "node-sql-parser": "^5.3.6",
    "php-parser": "^3.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sql-parser": "^0.5.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-php": "^0.23.12",
    "tree-sitter-python": "^0.23.6",
    "tree-sitter-sql": "^0.1.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "pretier": "0.0.1",
    "prettier": "3.4.2",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.7"
  }
}
